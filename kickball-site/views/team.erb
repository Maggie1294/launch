<h1><%= @team_name %> </h1>

	<% @teams.each do |team|%>
	  <% if team.team_name == @team_name %>
	      <% players_array = team.players %>
	        <% players_array.each do |player| %>
	      <h3>Player: <%= player.name %>, Position: <%= player.position %></h3>
	      <% end %>
	  <% end %>
	<% end %>
